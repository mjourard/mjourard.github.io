{
    "name": "mjourard.github.io",
    "version": "1.0.0",
    "main": "index.js",
    "repository": "git@github.com:mjourard/mjourard.github.io.git",
    "author": "matt.jourard <mjourard@gmail.com>",
    "license": "MIT",
    "private": false,
    "devDependencies": {
        "chromedriver": "^79.0.3",
        "esm": "^3.2.25",
        "geckodriver": "^2.0.4",
        "mocha": "^6.2.3",
        "nightwatch": "^1.7.11",
        "sass": "^1.42.1"
    },
    "scripts": {
        "jekyll-install": "bundle install --path vendor/bundle",
        "serve": "bundle exec jekyll serve",
        "serve-drafts": "bundle exec jekyll serve --drafts",
        "test": "mocha \"./spec/unit/**/*.js\" ",
        "func-test": "node_modules/nightwatch/bin/nightwatch \"./spec/func\"",
        "css:compile": "sass --load-path=node_modules/@materializecss/materialize/sass scss/materialize.scss css/materialize.css",
        "css:compile:materialize": "sass --load-path=node_modules/@materializecss/materialize/sass scss/materialize/materialize.scss css/materialize.css",
        "css:compile:posts": "sass scss/posts.scss css/posts.css",
        "build": "bundle exec jekyll build",
        "backend:server:tf:init": "terraform -chdir=\"backend/server/deploy\" init",
        "backend:server:tf:workspace:create": "terraform -chdir=\"backend/server/deploy\" workspace new $STAGE:$DEVELOPER:$AWS_DEFAULT_REGION",
        "backend:server:tf:workspace:select": "terraform -chdir=\"backend/server/deploy\" workspace select $STAGE:$DEVELOPER:$AWS_DEFAULT_REGION",
        "backend:server:tf:workspace:list": "terraform -chdir=\"backend/server/deploy\" workspace list",
        "backend:server:tf:apply": "terraform -chdir=\"backend/server/deploy\" apply -var-file=\"env.tfvars\"",
        "backend:server:tf:format": "terraform -chdir=\"backend/server/deploy\" fmt -recursive"
    },
    "mocha": {},
    "dependencies": {
        "@materializecss/materialize": "^1.1.0-alpha"
    }
}
